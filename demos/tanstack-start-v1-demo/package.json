{
  "name": "@spautz/skip-the-build--tanstack-start-v1-demo",
  "private": true,
  "description": "Tanstack-Start demo of skip-the-build, using the Vite adapter",
  "license": "MIT",
  "homepage": "https://github.com/spautz/skip-the-build/blob/main/demos/tanstack-start-v1-demo#readme",
  "bugs": "https://github.com/spautz/skip-the-build/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spautz/skip-the-build.git",
    "directory": "demos/tanstack-start-v1-demo"
  },
  "author": {
    "name": "Steven Pautz",
    "url": "https://stevenpautz.com"
  },
  "source": "./src/main.tsx",
  "type": "module",
  "scripts": {
    "____ STANDARD PACKAGE TASKS ________________________________________": "",
    "all": "turbo run pipeline-all",
    "all:ci": "turbo run pipeline-ci --ui=stream",
    "build": "pnpm run build:clean && pnpm run build:main",
    "clean": "pnpm run \"/^.*:clean$/\" && rimraf --glob ./node_modules/.cache ./.turbo ./*.log",
    "dev": "vite",
    "lint": "biome check --write .",
    "lint:verify": "biome check .",
    "start": "vite preview",
    "test:clean": "rimraf ./coverage",
    "test_SKIPPED": "pnpm run test:clean       && vitest run --coverage",
    "test:ci_SKIPPED": "pnpm run test:clean    && vitest run --coverage --bail 1",
    "test:ui_SKIPPED": "pnpm run test:clean    && vitest --ui",
    "test:watch_SKIPPED": "pnpm run test:clean && vitest watch --coverage",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "____ BUILD & DEPLOY TASKS __________________________________________": "",
    "build:clean": "rimraf ./dist",
    "build:main": "vite build",
    "build:watch": "pnpm run build:clean && vite build --watch",
    "size-check": "source-map-explorer dist/**/*.js",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "storybook:clean": "rimraf ./storybook-static",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@spautz/slow-package-example": "workspace:",
    "@storybook/react-vite": "10.2.8",
    "@tailwindcss/vite": "4.1.18",
    "@tanstack/react-devtools": "0.9.5",
    "@tanstack/react-router": "1.159.4",
    "@tanstack/react-router-devtools": "1.159.4",
    "@tanstack/react-router-ssr-query": "1.159.4",
    "@tanstack/react-start": "1.159.4",
    "@tanstack/router-plugin": "1.159.4",
    "lucide-react": "0.563.0",
    "nitro": "npm:nitro-nightly@latest",
    "react": "catalog:",
    "react-dom": "catalog:",
    "storybook": "10.2.8",
    "tailwindcss": "4.1.18",
    "vite-tsconfig-paths": "6.0.2"
  },
  "devDependencies": {
    "@skip-the-build/vite": "workspace:",
    "@tanstack/devtools-vite": "0.5.1",
    "@testing-library/dom": "10.4.1",
    "@testing-library/react": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "5.1.3",
    "babel-plugin-react-compiler": "1.0.0",
    "jsdom": "28.0.0",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "web-vitals": "5.1.0"
  }
}
